varnishtest "Test geoip vmod"

server s1 {
        rxreq
        txresp
} -start

varnish v1 -vcl+backend {
        import geoip from "${vmod_topbuild}/src/.libs/libvmod_geoip.so";

        sub vcl_deliver {
                set resp.http.X-Geo-Country = geoip.country_code_v6("2001:550:6600:0:e533:aeab:63fe:dd94");
        }
} -start

client c1 {
        txreq -url "/"
        rxresp
        expect resp.http.X-Geo-Country == "US"
} -run

