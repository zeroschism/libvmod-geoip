varnishtest "Test geoip vmod"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
	import geoip from "${vmod_topbuild}/src/.libs/libvmod_geoip.so";

	sub vcl_deliver {
		set resp.http.X-Country-Name = geoip.country_code("85.10.240.255");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.X-Country-Name == "DE"
} -run
